{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6"],
    "module": "ES6",
    "noEmit": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowJs": true,
    "skipLibCheck": true,
    "noImplicitAny": false
  },
  "exclude": ["./examples/"],
  "typedocOptions": {
    "name": "IC-Naming JavaScript SDK",
    "excludeExternals": true,
    "excludePrivate": true,
    "readme": "./README.md",
    "out": "docs/"
  }
}
